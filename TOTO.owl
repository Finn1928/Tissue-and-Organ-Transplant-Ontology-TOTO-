        <terms:contributor rdf:resource="https://orcid.org/0009-0002-7282-0836"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-04-13T21:28:22Z</terms:created>
        <rdfs:comment>a vital function of a proper part of an organism that must be realized for an organism to be an integrated whole operating to resist entropy</rdfs:comment>
        <rdfs:label xml:lang="en">critical function</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/TOTO_0000005 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/TOTO_0000005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
