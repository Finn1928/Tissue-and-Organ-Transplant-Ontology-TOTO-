        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-04-13T17:32:29Z</terms:created>
        <rdfs:label xml:lang="en">life</rdfs:label>
        <skos:definition>a process in which an organic entity participates in bodily activities that make it an integrated whole operating to resist entropy</skos:definition>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/TOTO_0000001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/TOTO_0000001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000035"/>
        <terms:contributor rdf:resource="https://orcid.org/0009-0002-7282-0836"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-04-13T17:34:19Z</terms:created>
        <rdfs:label xml:lang="en">death</rdfs:label>
        <skos:definition>a process boundary that is an ending of a life process</skos:definition>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/TOTO_0000002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/TOTO_0000002">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000034"/>
        <terms:contributor rdf:resource="https://orcid.org/0009-0002-7282-0836"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-04-13T17:35:03Z</terms:created>
        <rdfs:label xml:lang="en">vital function</rdfs:label>
        <skos:definition>a function of a proper part of an organism that non-situationally must be realized for an organism to be an integrated whole operating to resist entropy, or is such that, if it is not realized, the loss of function will cause the organism to no longer be an integrated whole operating to resist entropy regardless of circumstance if the loss of function is not reversed or preempted</skos:definition>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/TOTO_0000003 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/TOTO_0000003">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/UBERON_0000062"/>
        <terms:contributor rdf:resource="https://orcid.org/0009-0002-7282-0836"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-04-13T21:27:08Z</terms:created>
        <rdfs:comment>an organ that would be necessary now or in the future for a vital function of an organism to be realized if the organ was functional and no other material entity were to realize the vital function</rdfs:comment>
        <rdfs:label xml:lang="en">vital organ</rdfs:label>
    </owl:Class>
    


